// ---------------------------------------------------------------------------

#ifndef QueueH
#define QueueH
#include <string>
#include "Node.h"

class Queue {
public:
	Node* head = NULL;
	Node* tail = NULL;
	void PushFront(std::string data);
	void PopBack();
	std::string GetFront();
	std::string GetBack();
	bool Empty();
};
// ---------------------------------------------------------------------------
#endif
