// ---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "QueueTest.h"
#include "Queue.h"
// ---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm1 *Form1;
Queue queue;

// ---------------------------------------------------------------------------
__fastcall TForm1::TForm1(TComponent* Owner) : TForm(Owner) {
	for (int i = 0; i < ListBox1->Count; i++) {
		queue.PushFront(AnsiString(ListBox1->Items->Strings[i]).c_str());
	}
	queue.Refresh(ListBox1);
}
// ---------------------------------------------------------------------------
